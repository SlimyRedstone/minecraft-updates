{
	"name": "Minecraft Updates",
	"version": "1.0.0",
	"description": "Shows the latest Minecraft JE Updates",
	"manifest_version": 3,
	"author": "SlimyRedstone",
	"permissions": ["alarms", "tabs", "activeTab", "storage", "webRequest", "notifications"],
	"background": {
		"service_worker": "background.js"
	},
	"action": {
		"default_popup": "popup.html"
	},
	"icons": {
		"16": "icons/16.png",
		"32": "icons/32.png",
		"48": "icons/48.png",
		"128": "icons/128.png"
	},
	"externally_connectable": {
		"matches": ["https://*.mojang.com/*"]
	},
	"host_permissions": ["https://*.mojang.com/*"]
}
